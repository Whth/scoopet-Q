{
    "version": "0.8.5",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.8.5/uv-x86_64-pc-windows-msvc.zip",
            "hash": "898049e2db4a92e59bf1da7e14cad9b5afadb7d0cf9dd55a5274825290f7ba6d"
        },
        "32bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.8.5/uv-i686-pc-windows-msvc.zip",
            "hash": "2c133809d2839eac93341a14df6fb16bf834ae3c7582f069cf24ccc60cf367f7"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "env_set": {
        "UV_INDEX": "https://mirrors.aliyun.com/pypi/simple/ https://mirrors.huaweicloud.com/repository/pypi/simple https://mirrors.tuna.tsinghua.edu.cn/pypi/web/simple",
        "UV_DEFAULT_INDEX": "https://mirrors.aliyun.com/pypi/simple/",
        "UV_TOOL_BIN_DIR": "$dir/bin",
