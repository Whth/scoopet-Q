{
    "version": "0.9.29",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.9.29/uv-x86_64-pc-windows-msvc.zip",
            "hash": "9825b1a5955d8a432b664e56660641aac8886ed30cd9c59a94aacc68ae9116ce"
        },
        "32bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.9.29/uv-i686-pc-windows-msvc.zip",
            "hash": "049a929882a3f4a2d054c9dc44848d2c24175079696e131a57d60d9ab62df81a"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "env_set": {
        "UV_INDEX": "https://mirrors.aliyun.com/pypi/simple/ https://mirrors.huaweicloud.com/repository/pypi/simple https://mirrors.tuna.tsinghua.edu.cn/pypi/web/simple",
