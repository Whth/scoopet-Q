            "hash": "93a0a244f26eec208d8ea8077e3de743d9687ad76c190342722f1f57fa629457"
        },
        "32bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.9.22/uv-i686-pc-windows-msvc.zip",
            "hash": "f08889e9567feb1e802c9e588de60efef5a436630e4c211fbf4a2116928f69ab"
        }
    },
    "bin": [
        "uv.exe",
