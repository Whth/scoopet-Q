{
    "version": "0.8.10",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.8.10/uv-x86_64-pc-windows-msvc.zip",
            "hash": "37fcd011fd22b2a569f7e583a924af2d624d99445f669752923a2fd3841f8e3d"
        },
        "32bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.8.10/uv-i686-pc-windows-msvc.zip",
            "hash": "e62a2549135b4b9474c18e7bc9f0937837012dd9039f6b4b2c109a1cf99ad925"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "env_set": {
        "UV_INDEX": "https://mirrors.aliyun.com/pypi/simple/ https://mirrors.huaweicloud.com/repository/pypi/simple https://mirrors.tuna.tsinghua.edu.cn/pypi/web/simple",
