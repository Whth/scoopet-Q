{
    "version": "0.9.7",
    "description": "An extremely fast Python package installer and resolver, written in Rust.",
    "homepage": "https://docs.astral.sh/uv/",
    "license": "Apache-2.0|MIT",
    "architecture": {
        "64bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.9.7/uv-x86_64-pc-windows-msvc.zip",
            "hash": "5d250c32d3604e28dbe18dc65c668ff628c53e00dde2c642576e831e4a60da64"
        },
        "32bit": {
            "url": "https://ghfast.top/https://github.com/astral-sh/uv/releases/download/0.9.7/uv-i686-pc-windows-msvc.zip",
            "hash": "02349c4380c26838be6fd9bde646c5c3296adc2ba224509ee8e16ffa5baba13e"
        }
    },
    "bin": [
        "uv.exe",
        "uvx.exe"
    ],
    "checkver": {
        "github": "https://github.com/astral-sh/uv"
    },
    "env_set": {
        "UV_INDEX": "https://mirrors.aliyun.com/pypi/simple/ https://mirrors.huaweicloud.com/repository/pypi/simple https://mirrors.tuna.tsinghua.edu.cn/pypi/web/simple",
